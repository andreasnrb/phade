{
    "name": "cyonite/phade",
    "description": "HAML-like template engine for PHP 5.5",
    "keywords": ["jade", "minification","template"],
    "homepage": "https://github.com/cyonite/phade",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Nurbo",
            "email": "andreas@cyonitesystems.com",
            "homepage": "http://cyonitesystems.com",
            "role": "Maintainer"
        }
    ],
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "cyonite/underscore.php",
          "version": "@dev",
          "source": {
            "type": "git",
            "url":  "git@github.com:cyonite/underscore.php.git",
            "reference": "master"
          }
        }
      }
    ],
    "require": {
        "php": ">=5.5.0",
        "cyonite/underscore.php":"@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Phade": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
